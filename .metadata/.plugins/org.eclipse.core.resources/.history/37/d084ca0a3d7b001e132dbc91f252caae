class M_array
{
	public static void main(String args[])
	{
		int arr[][] = new int [3][4];
		
		for(int i=0; i<4; i++)
		{
			for (int j=0; j<3; j++)
			{
				arr[i][j] = (int)(Math.random()*10);
			}
			System.out.println();
		}
		for (int i=0; i<3; i++)
		{
			for(int j=0; j<3; j++)
			{
				System.out.println(arr[i][j] + " ");
			}
			System.out.println();
		}
	}
}