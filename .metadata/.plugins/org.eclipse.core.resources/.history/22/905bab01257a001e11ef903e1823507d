
public class reverse_k
{
	public static void main(String args[])
	{
		int i,j,k;
		
		for (i=1; i<=5; i++)
		{
			for (j=i; j<=5; j++)
			{
				System.out.print(" ");
			}
			for (k=1; k<=i; k++)
			{
				System.out.print("*");
			}
			System.out.println();
		}
		for (i=1; i<=5; i++)
		{
			for(j=1; j<=i; i++)
			{
				System.out.print(" ");
			}
			for (k=i; k<=5; k++)
			{
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
