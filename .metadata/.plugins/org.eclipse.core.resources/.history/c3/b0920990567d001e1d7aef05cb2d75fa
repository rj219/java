
public class Calc {
	public static void main(String args[])
	{
		int a=20,b=30;
		
		System.out.println("The Summation of the value a + b is :" + (a+b));
		System.out.println("The Substraction of the value a  b is :" + (a - b));
		System.out.println("The Multiplicartion of the value a * b is:" + (a*b));
		System.out.println("The Division of the value a / b is :" + (a/b));
		System.out.println("The Modulus of the value a % b is :" + (a%b));
		
	}
}
